{
  "version":2,
  "expire":6,
  "rules":[
    {
      "site": "http://www.cocoachina.com/*",
      "hold": ".detail-main",
      "delete": [
        "#comiframe",
        ".part-wrap",
        ".wx_article",
        ".crumbs",
        ".tgbox",
        ".article-prev",
        ".article-next",
        "#zanbox",
        ".adsbox"
      ],
      "css": ".middle,.detail-left{width: auto !important;}.float-l{float: inherit}.info-detail, .detail-left{padding: 0;}.detail-left .detail-main{background-color:#fff;border: none}"
    },
    {
      "site": "http://www.justinyan.me/post/*",
      "hold": "article",
      "delete": [
        "footer",
        ".section_separator",
        ".wp-posturl"
      ],
      "css": "#content{margin: 0 1em;width: auto;} body{border-top:0;}"
    },
    {
      "site": "http://onevcat.com/*",
      "hold": "article",
      "delete": [
        ".post-comments",
        ".post-meta"
      ],
      "css": ".content-wrapper{margin-left: 0;width: auto;}.content-wrapper__inner{margin:1em;padding:0;}"
    },
    {
      "site": "http://blog.devtang.com/blog/*",
      "hold": "article.hentry",
      "delete": [
        "footer"
      ],
      "css": "#content{margin:0}body{max-width:inherit}body > header, body > nav, body > footer, body #content > article, body #content > div > article, body #content > div > section{padding:2em}"
    },
    {
      "site": "^http://www.raywenderlich.com/\\d+/\\S+",
      "hold": "article",
      "delete": [
        ".social-links"
      ],
      "css": "article{border:0;}html{background:#fff}#main{padding:0}.wrapper{max-width:inherit;width:auto;}article:hover, .rw-module:hover{box-shadow:none;}#primary-container,#primary{margin:0;float:none;}"
    },
    {
    "site":"^.*\\.jobbole\\.com\/\\d+/",
    "hold":".post",
    "delete":[
      ".textwidget",
      ".copyright-area",
      ".entry-meta",
      ".imooc",
      ".jiathis_style_24x24",
      "#author-bio"
    ],
    "css":".grid-8{float:none;margin:0;width:inherit;}.container{width:inherit;}"
  },
  {
    "site":"http://www.objc.io/*",
    "hold":"article",
    "delete":[
      ".issue-link",
      ".post-author"
    ],
    "css":".article{max-width: 100%;padding:2em;}"
  },
  {
    "site":"http://objccn.io/*",
    "delete":[
      "#at_header",
      "#at_footer",
      ".at_author",
      ".at_post_share",
      ".at_post_content+h3"
    ],
    "css":".row{max-width:100%;}"
  },
  {
    "site":"^http://nshipster.com/\\S+/",
    "hold":"article",
    "delete":[
      "footer"
    ],
    "css":"[role='container']{max-width:100%;}.st-search-launcher{display:none;}"
  },
  {
    "site":"^http://nshipster.cn/\\S+/",
    "hold":"article",
    "delete":[
      "footer"
    ],
    "css":"[role='container']{max-width:100%;}.st-search-launcher{display:none;}"
  },
  {
    "site":"^https://www.bignerdranch.com/blog/\\S+/",
    "delete":[
      "header.nav-skin-top-header",
      "footer",
      ".l-container.u-tac.l-padding-bottom",
      ".bg-fence-border-tile",
      "main section:nth-last-child(2)",
      ".list-c-social-blog",
      ".blog-t-category",
      ".blog-post-l-container"
    ],
    "css":".l-shift-grid, .blog-post-l-container, .media-l-container{margin-left:0;width:inherit;}"
  },
  {
    "site":"^http://security.ios-wiki.com/\\S+/",
    "delete":[
      "#at_header",
      "#at_footer",
      ".at_author",
      ".at_post_share",
      ".at_post_content+h3"
    ],
    "css":".row{max-width:100%;}"
  },
  {
    "site":"^http://blog.csdn.net/\\S+/.*",
    "hold":"#article_details",
    "delete":[
      ".bdsharebuttonbox",
      ".article_next_prev",
      "#digg",
      ".article_manage",
      ".tag2box"
    ],
    "css":"#main,#main .main{width:inherit;max-width:100%; border:0;padding: 0;margin:0;}body{background-image:none;background-color:#fff}#body{width:100%;max-width:100%;padding: 0;margin:0;border:0;}"
  }
  ]
}